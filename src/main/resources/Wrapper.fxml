<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="new_design.Controller">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Project">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#newProject" text="New project" />
                  <MenuItem mnemonicParsing="false" onAction="#openProject" text="Open project" />
                  <MenuItem mnemonicParsing="false" onAction="#cloneProject" text="Clone project" />
                  <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Module">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#newModule" text="New module" />
                  <MenuItem mnemonicParsing="false" onAction="#editModule" text="Edit module" />
                  <MenuItem mnemonicParsing="false" onAction="#removeModule" text="Remove module" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Test">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#newTest" text="New test" />
                  <MenuItem mnemonicParsing="false" onAction="#editTest" text="Edit test" />
                  <MenuItem mnemonicParsing="false" onAction="#removeTest" text="Remove test" />
               </items>
            </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <top>
                  <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="380.0" BorderPane.alignment="CENTER">
                     <items>
                        <Button mnemonicParsing="false" onAction="#push" text="push" />
                        <Button mnemonicParsing="false" onAction="#commit" text="commit" />
                        <Button mnemonicParsing="false" onAction="#run" text="run" />
                     </items>
                  </ToolBar>
               </top>
               <left>
                  <TreeView fx:id="TreeView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
               </left>
            </BorderPane>
         </children></AnchorPane>
  </children>
</VBox>
